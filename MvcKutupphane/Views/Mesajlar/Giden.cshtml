@using MvcKutupphane.Models.Entity;
@model List<TblMesajlar>
@{
    ViewBag.Title = "Giden";
    Layout = "~/Views/Shared/_LayoutPanelim.cshtml";
}


<h2>Giden Mesajlar Sayfası</h2>
<br />
<table class="table table-bordered">
    <tr>
        <th>Alıcı</th>
        <th>Konu</th>
        <th>Mesaj</th>
        <th>Tarih</th>
    </tr>
    @foreach (var x in Model)
    {
        <tr>
            <td>@x.Alıcı </td>
            <td>@x.Konu </td>
            <td>@x.Icerik </td>
            <td>@x.Tarih </td>

        </tr>

    }
</table>


